	<!-- 快捷鍵設定 -->
	<!-- 開啟遊戲內控制台（預設值為 F4） -->
	<config id="ConsoleKey" value="0"/>
	<!-- 翻譯插件開關（中英切換） （預設值為 F11） -->
	<config id="EnableTranslationKey" value="1A"/>
	<!-- 輸出畫面中文字翻譯的狀況（預設值為 F12） -->
	<config id="TranslationLoggingKey" value="1B"/>
	<!-- 是否要開關 FPS 管理功能，預設未開啟 -->
	<config id="FPSManagementKey" value="0"/>
	<!-- 遊戲中重載字型的快捷鍵（預設值為 F10）-->
	<config id="ReloadingFontKey" value="28"/>

	<!-- 翻譯設定 -->
	<!-- 1: 所有文字均透過 FOZH 繪製 / 0: 只有翻譯成功的字串透過 FOZH 繪製 -->
	<config id="AllTextReplace" value="1"/>
	<!-- 調整文字的垂直位置。正數值表示向下，負數值表示向上。 -->
	<config id="LineAdjuster" value="0"/>
	<!-- FOZH 要使用的字元集。日文：128、韓文：129、簡體中文（GBK）：134、繁體中文（Big-5）：136  -->
	<config id="CharacterSet" value="134"/>
	<!-- 1: 當字典找不到完整匹配的字串時，透過規則修正原文文字。 / 0: 關閉 -->
	<config id="CorrectDictionaryText" value="1"/>
	<!-- 1: 修正 Xbox 控制器圖示包括在文字內時，無法正確顯示的問題。開啟有可能會導致遊戲當機。字典需做相對應的修正 / 0: 關閉 -->
	<config id="FixXboxIconIssue" value="0"/>
	<!-- 1: 總是輸出所有文字的翻譯處理情況至 fozh.log，若開啟會稍微增加遊戲負擔 / 0: 關閉 -->
	<config id="AlwaysLoggingText" value="0"/>
	<!-- New in FOZH v1.6.0 -->
	<!-- 1: 啟用遊戲內電臺字幕（僅在 NVSE 加載下可用） / 0: 關閉 -->
	<config id="EnableRadioSubtitles" value="1"/>

	<!-- FPS 管理 -->
	<config id="FPSManagement" value="0"/>
	<!-- 最大 FPS -->
	<!-- 1: 限制最大 FPS / 0: 不限制 -->
	<config id="FPSMaximum" value="0"/>

	<!-- d3d9.dll 設置 -->
	<!-- 指定 FOZH 要同時載入的 DLL 名稱，僅限 d3d9.dll 模式下有效。這設定類似於 ENB 的 ProxyLibrary 設定 -->
	<config id="ProxyLibraryPath" value=""/>
	<!-- 1: 將圖形處理委派給指定的 ProxyLibrary DLL 處理 / 0: 關閉。這設定類似於 ENB 的 InitProxyFunctions -->
	<config id="LoadDLLAsD3D9" value="1"/>

	<!-- 雜項設置  -->
	<!-- 1: 避免 New Vegas 中的搖晃狀況（？） / 0: 關閉 -->
	<config id="ForceDirect3D9" value="1"/>
	<!-- 1: 阻止材質從 VRAM 中複製至主記憶體中，來減少記憶體用量。 / 0: 關閉 -->
	<config id="ApplyMemoryPatch" value="1"/>

